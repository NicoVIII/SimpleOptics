root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.yml]
indent_size = 2

[*.fsproj]
indent_size = 2

[*.{fs,fsx,fsi}]
indent_size = 4
fsharp_multiline_bracket_style = stroustrup
fsharp_keep_max_number_of_blank_lines = 1
